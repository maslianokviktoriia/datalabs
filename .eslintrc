{ 
  "parser": "babel-eslint",
  "extends": "eslint-config-airbnb",
  "env": {
    "browser": true,
    "mocha": true
  },
  "rules": {
    "no-case-declarations": 0,
  },
  "plugins": [
    "babel",
    "react"
  ],
  "settings": {
    "import/resolve": {
      moduleDirectory: ["node_modules"]
    }
  }
}
